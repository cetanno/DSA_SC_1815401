swap = input()
pos = 1
for letter in swap:
    if letter == 'A':
        if pos == 1:
            pos = 2
        elif pos == 2:
            pos = 1
    if letter == 'B':
        if pos == 3:
            pos = 2
        elif pos == 2:
            pos = 3
    if letter == 'C':
        if pos == 3:
            pos = 1
        elif pos == 1:
            pos = 3
print(pos)
